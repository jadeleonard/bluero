generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model about {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  about        String?
  aboutContent String?
  content      String?
  section      String?
}

model bluero {
  id              String  @id @default(auto()) @map("_id") @db.ObjectId
  content         String?
  herocta         String?
  herodescription String?
  section         String?
}

model button {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  button        String?
  buttonContent String?
  buttonTitle   String?
  content       String?
  tutorial1     String?
  tutorial2     String?
}

model faq {
  id                  String  @id @default(auto()) @map("_id") @db.ObjectId
  faq                 String?
  faqCustomize        String?
  faqCustomizeConent  String?
  faqCustomizeContent String?
  faqcontent          String?
  faqcontentDL        String?
  faqdownload         String?
  faqtitle            String?
}

model input {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  content String
  section String
}

model tutorial {
  id         String  @id @default(auto()) @map("_id") @db.ObjectId
  components String?
  const      String?
  content    String?
  import     String?
  section    String?
}
